{
  "name": "multisig-wallet",
  "version": "1.0.0",
  "description": "implementation of multisignature wallet",
  "scripts": {
    "start": "npx hardhat node",
    "test": "npx hardhat test",
    "deploy:localhost": "npx hardhat run ./scripts/dev.deploy.ts --network localhost",
    "deploy:hardhat": "npx hardhat run ./scripts/dev.deploy.ts --network hardhat",
    "deploy:ropsten": "npx hardhat run ./scripts/deploy.ts --network ropsten",
    "deposit:localhost": "npx hardhat run ./scripts/deposit.ts --network localhost",
    "data:localhost": "npx hardhat run ./scripts/contract.data.ts --network localhost",
    "create:localhost": "npx hardhat run ./scripts/contract.create-transaction.ts --network localhost",
    "confirm:localhost": "npx hardhat run ./scripts/contract.confirm-transaction.ts --network localhost",
    "revoke:localhost": "npx hardhat run ./scripts/contract.revoke-transaction.ts --network localhost",
    "execute:localhost": "npx hardhat run ./scripts/contract.execute-transaction.ts --network localhost",
    "wallet:setup:localhost": "npx hardhat wallet:setup --network localhost",
    "wallet:setup:hardhat": "npx hardhat wallet:setup --network hardhat",
    "docs": "npx hardhat docgen",
    "etherscan:verify:ropsten": "npx hardhat verify --constructor-args etherscan-args.js --network ropsten 0xefFFC131E490494cce8666e8663C5B6C106B9B99",
    "tenderly:verify:ropsten": "npx hardhat tenderly:verify Multisig=0xefFFC131E490494cce8666e8663C5B6C106B9B99 --network ropsten",
    "tenderly:push:ropsten": "npx hardhat tenderly:push Multisig=0xefFFC131E490494cce8666e8663C5B6C106B9B99 --network ropsten"
  },
  "author": "Pavol Karas",
  "license": "ISC",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@tenderly/hardhat-tenderly": "^1.0.12",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.0.2",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.4",
    "@types/prompts": "^2.0.14",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.3.1",
    "hardhat": "^2.4.0",
    "hardhat-docgen": "^1.1.1",
    "hardhat-gas-reporter": "^1.0.4",
    "prompts": "^2.4.1",
    "solhint": "^3.3.6",
    "ts-node": "^10.0.0",
    "typechain": "^5.0.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {}
}
